{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "ESNext",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "build",
    "baseUrl": ".",
    "skipLibCheck": true,
    "allowJs": true,
    // "types": [],
    "paths": {
      "*": ["node_modules/*", "src/types/*", "src2/types/*"]
    }
  }
}
